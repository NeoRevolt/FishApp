<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fishapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FishApp1">
        <activity
            android:name=".KesehatanIkanActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".BersihAquariumActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".KategoriActivity"
            android:parentActivityName=".SeluruhKategoriActivity" />
        <activity
            android:name=".recyclecari.TampilDataIkanActivity"
            android:parentActivityName=".recyclecari.ListIkanActivity" />
        <activity
            android:name=".recyclecari.ListIkanActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".IkanInvasifActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SeluruhKategoriActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".InfoApp.InfoActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>